    <servlet-class>mod4.DateServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DateServlet</servlet-name>
    <url-pattern>/dateservlet</url-pattern>
  </servlet-mapping>

<!-- mod5課題 -->
